{
    "name_patterns": [
        {
            "pattern": "(채무자|신청인)([가-힣]{2,4})\\d{6}-\\d{7}",
            "group": 2
        },
        {
            "pattern": "(채무자|신청인)([가-힣]{2,4})\\(",
            "group": 2
        },
        {
            "pattern": "([가-힣]{2,4})\\(\\d{6}-\\d{7}\\)",
            "group": 1
        },
        {
            "pattern": "(채무자|신청인)\\s*([가-힣]{2,4})",
            "group": 2
        }
    ],
